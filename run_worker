REDIS_HOST=192.168.1.40
if [[ $1 == "--proxy" || $2 == "--proxy" ]]
then 
    cd proxy_pool/Run
    lsof -i:5010|grep python|awk '{print $2}'|xargs kill -9
    PROXY_BACKEND_HOST=$REDIS_HOST python main.py > proxy.log 2>&1 &
    cd ../../
fi
if [[ $1 == "exe" ]]
then
    CELERY_BROKER_HOST=$REDIS_HOST CELERY_BACKEND_HOST=$REDIS_HOST CMD=worker ./creeper
else
    CELERY_BROKER_HOST=$REDIS_HOST CELERY_BACKEND_HOST=$REDIS_HOST CMD=worker go run .
fi
